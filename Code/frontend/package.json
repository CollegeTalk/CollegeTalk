{
    "name": "frontend",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "postinstall": "patch-package",
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "lint": "cross-env RN_ENV=development eslint './**/*.{js,ts,jsx,tsx}' --fix",
        "lintProd": "cross-env RN_ENV=production eslint './**/*.{js,ts,jsx,tsx}' --quiet --fix",
        "test": "jest --watch --coverage=false",
        "testProd": "jest",
        "updateSnapshots": "jest -u --coverage=false",
        "publish": "expo publish"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@rneui)"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{js,jsx,ts,tsx}",
            "!**/__tests__/**",
            "!**/coverage/**",
            "!**/node_modules/**",
            "!**/react-native.config.js",
            "!**/babel.config.js",
            "!**/types.tsx",
            "!**/*.{json}",
            "!**/.*"
        ]
    },
    "dependencies": {
        "@expo/vector-icons": "^12.0.0",
        "@react-navigation/bottom-tabs": "^6.0.5",
        "@react-navigation/drawer": "^6.4.1",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/native-stack": "^6.1.0",
        "@react-navigation/stack": "^6.2.1",
        "@rneui/base": "^4.0.0-rc.1",
        "@rneui/themed": "^4.0.0-rc.1",
        "@types/uuid": "^8.3.4",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react-native": "^4.0.0",
        "expo": "~44.0.0",
        "expo-asset": "~8.4.4",
        "expo-constants": "~13.0.0",
        "expo-dev-client": "~0.8.5",
        "expo-font": "~10.0.4",
        "expo-linking": "~3.0.0",
        "expo-splash-screen": "~0.14.0",
        "expo-status-bar": "~1.2.0",
        "expo-updates": "~0.11.7",
        "expo-web-browser": "~10.1.0",
        "lottie-ios": "^3.2.3",
        "lottie-react-native": "^5.0.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-native": "^0.64.3",
        "react-native-gesture-handler": "~2.1.0",
        "react-native-get-random-values": "^1.7.2",
        "react-native-reanimated": "~2.3.1",
        "react-native-safe-area-context": "3.3.2",
        "react-native-screens": "~3.10.1",
        "react-native-select-dropdown": "^1.13.0",
        "react-native-web": "0.17.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/plugin-transform-flow-strip-types": "^7.16.7",
        "@babel/preset-react": "^7.16.7",
        "@types/jest": "^27.4.1",
        "@types/react": "~17.0.21",
        "@types/react-native": "~0.64.12",
        "@types/react-navigation": "^3.4.0",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "babel-plugin-inline-dotenv": "^1.7.0",
        "eslint": "^8.12.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.1.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.5.0",
        "jest": "^26.6.3",
        "jest-expo": "~44.0.1",
        "patch-package": "^6.4.7",
        "react-test-renderer": "17.0.1",
        "typescript": "~4.3.5"
    },
    "private": true
}
